G,EATJ,YAAA,CAQc,sBARd,EAQc;AAPX,MAAA,EAAE,EAAA;6BAAA;;gBAA6E,OAAO,CAAC;;AAArF,OAOS;AAHZ,MAAA,KAAK,EAAC;AAGM,KARd,E;wBAOE;AAAA,eAAyC,CAAzC,YAAA,CAAyC,sBAAzC,EAAyC;AAA3B,UAAA,IAAI,EAAE,OAAO,CAAC;AAAa,SAAzC,E,IAAA,E;;AAAA,U,QAAA,CAAyC,CAAzC;AAAA,O;;;;KAPF,E;;AAAA,M,MAAA,CASI,CAlBN,C,EAmBA,mBAAA,CA6BM,KA7BN,EAAA,WAAA,EA6BM,CA5BJ,mBAAA,CAAA,0WAAA,CA4BI,EArBJ,mBAAA,CAQK,IARL,EAAA,WAAA,EAQK,CAPH,YAAA,CAMC,sBAND,EAMC;AALE,MAAA,EAAE,EAAA;6BAAA;;gBAAiF,OAAO,CAAC;;AAAzF;AAKJ,KAND,E;wBAKG;AAAA,eAAkB,C,kCAAf,OAAO,CAAC,I,GAAI;AAAA;SAAG,CAAlB;AAAA,O;;;;KALH,E;;AAAA,M,MAAA,CAOG,CARL,CAqBI,EAZJ,YAAA,CAGE,wBAHF,EAGE;AAFC,MAAA,KAAK,EAAE,OAAO,CAAC,KAEhB;AADC,MAAA,QAAQ,EAAE,OAAO,CAAC;AACnB,KAHF,E,IAAA,E;;AAAA,M,qBAAA,CAYI,EAPJ,mBAAA,CAMS,QANT,EAMS;AALP,MAAA,KAAK,EAAC,aAKC;AAJP,MAAA,KAAK,EAAC,aAIC;AAHN,MAAA,OAAK,EAAA,cAAA,CAAA,UAAA,MAAA;AAAA,eAAU,QAAA,CAAA,SAAA,CAAU,OAAV,CAAV;AAAA,OAAA,EAA2B,CAAA,SAAA,CAA3B;AAGC,KANT,E,WAAA,E;;AAAA,M,WAAA,CAOI,CA7BN,C,CAtBF,E;;AAAA,K,CANF,C;GA2DM,CA3DN,E;;AAAA,GA4DI,EA7DN,CAqEI,EANmB,QAAA,CAAA,mBAAA,CAAoB,MAApB,IAA0B,C,kBAAjD,mBAAA,CAKM,KALN,EAAA,WAAA,EAKM,WALN,C,qCAMI,CAvGN,C","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"row\">\r\n      <div class=\"col-lg-12\">\r\n        <div class=\"top-filter\">\r\n          <div class=\"filter-show\">\r\n            <label class=\"filter-label\">Show :</label\r\n            ><select class=\"form-select filter-select\">\r\n              <option value=\"20\">20</option>\r\n              <option value=\"40\">40</option>\r\n              <option value=\"100\">100</option>\r\n            </select>\r\n          </div>\r\n          <div class=\"filter-short\">\r\n            <input\r\n              class=\"shop-widget-search\"\r\n              type=\"text\"\r\n              v-model=\"productSearch\"\r\n              placeholder=\"Search...\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"filter-short\">\r\n            <label class=\"filter-label\">Short by :</label\r\n            ><select class=\"form-select filter-select\">\r\n              <option selected>default</option>\r\n              <option value=\"3\">trending</option>\r\n              <option value=\"1\">featured</option>\r\n              <option value=\"2\">recommend</option>\r\n            </select>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"row row-cols-2 row-cols-md-3 row-cols-lg-3 row-cols-xl-4\">\r\n      <div\r\n        class=\"col\"\r\n        v-for=\"(product, index) in productSearchFilter\"\r\n        :key=\"index\"\r\n      >\r\n        <!-- //disable or stock out products // use class product-disable // after product-card -->\r\n        <div\r\n          :class=\"`product-card ${product.stock == 0 ? 'product-disable' : ''}`\"\r\n        >\r\n          <div class=\"product-media\">\r\n            <div class=\"product-label\">\r\n              <label class=\"label-text sale\">{{ product.conditions }}</label>\r\n              <label class=\"view-label off\" v-if=\"product.discount\"\r\n                >-{{ product.discount }}%</label\r\n              >\r\n            </div>\r\n             <add-to-wish-list :product=\"product\" />\r\n             \r\n            <router-link\r\n              :to=\"{\r\n                name: 'SingleProduct',\r\n                params: { slug: product.slug },\r\n              }\"\r\n              class=\"product-image\"\r\n            >\r\n              <Dynamic-img :path=\"product.thumbnail\" />\r\n            </router-link>\r\n          </div>\r\n          <div class=\"product-content\">\r\n            <!-- <div class=\"product-rating\">\r\n              <i class=\"active icofont-star\"></i\r\n              ><i class=\"active icofont-star\"></i\r\n              ><i class=\"active icofont-star\"></i\r\n              ><i class=\"active icofont-star\"></i><i class=\"icofont-star\"></i\r\n              ><a href=\"product-video.html\">(3)</a>\r\n            </div> -->\r\n            <h6 class=\"product-name\">\r\n              <router-link\r\n                :to=\"{\r\n                  name: 'SingleProduct',\r\n                  params: { slug: product.slug },\r\n                }\"\r\n                >{{ product.name }}</router-link\r\n              >\r\n            </h6>\r\n            <Product-price\r\n              :price=\"product.price\"\r\n              :discount=\"product.discount\"\r\n            />\r\n\r\n            <button\r\n              class=\"product-add\"\r\n              title=\"Add to Cart\"\r\n              @click.prevent=\"addToCart(product)\"\r\n            >\r\n              <i class=\"fas fa-shopping-basket\"></i><span>Shop Now</span>\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"row\" v-if=\"productSearchFilter.length == 0\">\r\n      <h6 class=\"text-center\">\r\n        Search No Result We're sorry. <br />\r\n        We cannot find any matches for your search term.\r\n      </h6>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport DynamicImg from \"@/components/frontend/DynamicImg.vue\";\r\nimport ProductPrice from \"@/components/frontend/ProductPrice.vue\";\r\nimport AddToWishList from \"@/components/frontend/AddToWishList.vue\";\r\nimport { ElNotification } from \"element-plus\";\r\nexport default {\r\n  components: { DynamicImg, ProductPrice,AddToWishList },\r\n  props: [\"products\"],\r\n\r\n  data() {\r\n    return {\r\n      productSearch: \"\",\r\n    };\r\n  },\r\n  computed: {\r\n    user() {\r\n      return this.$store.getters[\"auth/GET_AUTH_INFO\"];\r\n    },\r\n\r\n    productSearchFilter() {\r\n      return this.products.data.data.filter((product) => {\r\n        return product.name.match(this.productSearch);\r\n      });\r\n    },\r\n  },\r\n\r\n  methods: {\r\n     addToCart(product) {\r\n      if (product.discount) {\r\n        var numVal1 = product.price;\r\n        var numVal2 = product.discount / 100;\r\n        var totalValue = numVal1 - numVal1 * numVal2;\r\n        this.price = totalValue.toFixed();\r\n      } else {\r\n        this.price = product.price;\r\n      }\r\n\r\n      this.$store.dispatch(\"cart/ADD_OR_INCREMENT_CART\", {\r\n        id: product.id,\r\n        name: product.name,\r\n        price: this.price,\r\n        thumbnail: product.thumbnail,\r\n      });\r\n      ElNotification.success({\r\n        title: \"Success\",\r\n        message: \"Successfully Added On Cart\",\r\n        offset: 100,\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style></style>\r\n"],"sourceRoot":""}]}