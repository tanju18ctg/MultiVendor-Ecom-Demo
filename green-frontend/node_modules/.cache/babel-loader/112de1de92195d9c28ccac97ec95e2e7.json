AY,QAAZ,CAAqB,0BAArB;AACD,GAbY;AAeb,EAAA,QAAQ,EAAE;AACR,IAAA,SADQ,uBACI;AACV,aAAO,KAAK,MAAL,CAAY,OAAZ,CAAoB,wBAApB,CAAP;AACD;AAHO,GAfG;AAqBb,EAAA,OAAO,EAAE;AACP,IAAA,cADO,0BACQ,GADR,EACa;AAAA;;AAClB,WAAK,OAAL,GAAe,GAAf,CADkB,CAElB;;AACA,WAAK,MAAL,CACG,QADH,CACY,0BADZ,EACwC,GADxC,EAEG,IAFH,CAEQ,YAAM;AACV,QAAA,cAAc,CAAC,OAAf,CAAuB;AACrB,UAAA,KAAK,EAAE,SADc;AAErB,UAAA,OAAO,EAAE,iBAFY;AAGrB,UAAA,MAAM,EAAE;AAHa,SAAvB;AAKD,OARH,EASG,KATH,CASS,YAAM,CAAE,CATjB,EAUG,OAVH,CAUW,YAAM;AACb,QAAA,KAAI,CAAC,OAAL,GAAe,CAAf;AACD,OAZH;AAaD,KAjBM;AAmBP,IAAA,SAnBO,qBAmBG,OAnBH,EAmBY;AACjB,UAAI,OAAO,CAAC,QAAZ,EAAsB;AACpB,YAAI,OAAM,GAAI,OAAO,CAAC,KAAtB;AACA,YAAI,OAAM,GAAI,OAAO,CAAC,QAAR,GAAmB,GAAjC;AACA,YAAI,UAAS,GAAI,OAAM,GAAI,OAAM,GAAI,OAArC;AACA,aAAK,KAAL,GAAa,UAAU,CAAC,OAAX,EAAb;AACF,OALA,MAKO;AACL,aAAK,KAAL,GAAa,OAAO,CAAC,KAArB;AACF;;AAEA,WAAK,MAAL,CAAY,QAAZ,CAAqB,4BAArB,EAAmD;AACjD,QAAA,EAAE,EAAE,OAAO,CAAC,EADqC;AAEjD,QAAA,IAAI,EAAE,OAAO,CAAC,IAFmC;AAGjD,QAAA,KAAK,EAAE,KAAK,KAHqC;AAIjD,QAAA,SAAS,EAAE,OAAO,CAAC;AAJ8B,OAAnD;AAMA,MAAA,cAAc,CAAC,OAAf,CAAuB;AACrB,QAAA,KAAK,EAAE,SADc;AAErB,QAAA,OAAO,EAAE,4BAFY;AAGrB,QAAA,MAAM,EAAE;AAHa,OAAvB;AAKD;AAxCM;AArBI,CAAf","sourcesContent":["<template>\r\n  <div>\r\n    <section\r\n      class=\"inner-section single-banner\"\r\n      style=\"\r\n        background: url(//website//website/images/single-banner.jpg) no-repeat\r\n          center;\r\n      \"\r\n    >\r\n      <div class=\"container\">\r\n        <h2>wishlist</h2>\r\n        <ol class=\"breadcrumb\">\r\n          <li class=\"breadcrumb-item\"><a href=\"index.html\">My Wishlist</a></li>\r\n        </ol>\r\n      </div>\r\n    </section>\r\n    <section class=\"inner-section wishlist-part\">\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <div class=\"col-lg-12\">\r\n            <div class=\"table-scroll\">\r\n              <table class=\"table-list\" v-if=\"wishlists.data\">\r\n                <thead>\r\n                  <tr>\r\n                    <th scope=\"col\">Serial</th>\r\n                    <th scope=\"col\">Product</th>\r\n                    <th scope=\"col\">Name</th>\r\n                    <th scope=\"col\">Price</th>\r\n                    <th scope=\"col\">shopping</th>\r\n                    <th scope=\"col\">action</th>\r\n                  </tr>\r\n                </thead>\r\n\r\n\r\n                <tbody>\r\n                  <tr v-for=\"(wish, index) in wishlists.data\" :key=\"index\">\r\n                    <td class=\"table-serial\">\r\n                      <h6>{{ index + 1 }}</h6>\r\n                    </td>\r\n                    <td class=\"table-image\">\r\n                      <img\r\n                        :src=\"$filters.makeImagePath(wish.thumbnail)\"\r\n                        alt=\"product\"\r\n                      />\r\n                    </td>\r\n                    <td class=\"table-name\">\r\n                      <h6>{{ wish.name }}</h6>\r\n                    </td>\r\n                    <td class=\"table-price\">\r\n                      <h6 class=\"product-price\" v-if=\"wish.offer_price\">\r\n                        <del>{{ $filters.currencyBDT(wish.price) }}</del\r\n                        ><span>{{\r\n                          $filters.currencyBDT(wish.offer_price)\r\n                        }}</span>\r\n                      </h6>\r\n                      <h6 class=\"product-price\" v-else>\r\n                        <span>{{ $filters.currencyBDT(wish.price) }}</span>\r\n                      </h6>\r\n                    </td>\r\n\r\n                    <td class=\"table-shop\">\r\n                      <button\r\n                        class=\"product-add\"\r\n                        title=\"Add to Cart\"\r\n                        @click.prevent=\"addToCart(wish)\"\r\n                      >\r\n                        add to cart\r\n                      </button>\r\n                      <!-- fas fa-spinner fa-spin -->\r\n                    </td>\r\n                    <td class=\"table-action\">\r\n                      \r\n                      <a\r\n                        class=\"\"\r\n                        href=\"javascript::void(0)\"\r\n                        @click.prevent=\"deleteWishlist(wish.id)\"\r\n                        title=\"Remove Wishlist\"\r\n                      >\r\n                        <i\r\n                          class=\"fas fa-spinner fa-spin\"\r\n                          v-if=\"loading == wish.id\"\r\n                        ></i>\r\n                        <i class=\"icofont-trash\" v-else></i>\r\n                      </a>\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n              <template v-else>\r\n                <table-skeleton></table-skeleton>\r\n              </template>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"row\">\r\n          <div class=\"col-lg-12\">\r\n            <div class=\"text-center mt-5\">\r\n              <button class=\"btn btn-outline\">Load more items</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { ElNotification } from \"element-plus\";\r\nimport TableSkeleton from \"../../../components/skeleton/TableSkeleton.vue\";\r\n\r\nexport default {\r\n  name: \"Wishlist\",\r\n  components: { TableSkeleton },\r\n\r\n  data() {\r\n    return {\r\n      loading:0,\r\n    };\r\n  },\r\n\r\n  created() {\r\n    document.title = \"Wishlist\";\r\n    this.$store.dispatch(\"wishlist/FETCH_WISHLISTS\");\r\n  },\r\n\r\n  computed: {\r\n    wishlists() {\r\n      return this.$store.getters[\"wishlist/GET_WISHLISTS\"];\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    deleteWishlist(pId) {\r\n      this.loading = pId;\r\n      // this.btn_loading = true;\r\n      this.$store\r\n        .dispatch(\"wishlist/DELETE_WISHLIST\", pId)\r\n        .then(() => {\r\n          ElNotification.success({\r\n            title: \"Success\",\r\n            message: \"Product Removed\",\r\n            offset: 100,\r\n          });\r\n        })\r\n        .catch(() => {})\r\n        .finally(() => {\r\n          this.loading = 0;\r\n        });\r\n    },\r\n\r\n    addToCart(product) {\r\n      if (product.discount) {\r\n        var numVal1 = product.price;\r\n        var numVal2 = product.discount / 100;\r\n        var totalValue = numVal1 - numVal1 * numVal2;\r\n        this.price = totalValue.toFixed();\r\n      } else {\r\n        this.price = product.price;\r\n      }\r\n\r\n      this.$store.dispatch(\"cart/ADD_OR_INCREMENT_CART\", {\r\n        id: product.id,\r\n        name: product.name,\r\n        price: this.price,\r\n        thumbnail: product.thumbnail,\r\n      });\r\n      ElNotification.success({\r\n        title: \"Success\",\r\n        message: \"Successfully Added On Cart\",\r\n        offset: 100,\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style lang=\"\"></style>\r\n"],"sourceRoot":""}]}