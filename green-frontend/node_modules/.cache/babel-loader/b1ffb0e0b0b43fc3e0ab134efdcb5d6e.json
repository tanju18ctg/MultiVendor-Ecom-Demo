value=\"3\">36</option>\r\n            </select>\r\n          </div>\r\n          <div class=\"filter-short\">\r\n            <input\r\n              class=\"shop-widget-search\"\r\n              type=\"text\"\r\n              v-model=\"productSearch\"\r\n              placeholder=\"Search...\"\r\n            />\r\n          </div>\r\n\r\n          <div class=\"filter-short\">\r\n            <label class=\"filter-label\">Short by :</label\r\n            ><select class=\"form-select filter-select\">\r\n              <option selected>default</option>\r\n              <option value=\"3\">trending</option>\r\n              <option value=\"1\">featured</option>\r\n              <option value=\"2\">recommend</option>\r\n            </select>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"row row-cols-2 row-cols-md-3 row-cols-lg-3 row-cols-xl-4\">\r\n      <div\r\n        class=\"col\"\r\n        v-for=\"(product, index) in products().data.data\"\r\n        :key=\"index\"\r\n      >\r\n        <!-- //disable or stock out products // use class product-disable // after product-card -->\r\n        <div class=\"product-card\">\r\n          <div class=\"product-media\">\r\n            <div class=\"product-label\">\r\n              <label class=\"label-text sale\">{{ product.conditions }}</label>\r\n            </div>\r\n\r\n            <button\r\n              class=\"product-wish wish\"\r\n              v-if=\"user.name\"\r\n              @click.prevent=\"addToWishlist(product.id)\"\r\n            >\r\n              <i class=\"fas fa-heart\"></i>\r\n              <!-- added class is active class  -->\r\n            </button>\r\n\r\n            <button\r\n              class=\"product-wish wish\"\r\n              data-bs-toggle=\"modal\"\r\n              data-bs-target=\"#login-modal\"\r\n              v-else\r\n            >\r\n              <i class=\"fas fa-heart\"></i>\r\n            </button>\r\n            <router-link\r\n              :to=\"{\r\n                name: 'SingleProduct',\r\n                params: { slug: product.slug },\r\n              }\"\r\n              class=\"product-image\"\r\n              ><img :src=\"product.thumbnail\" alt=\"product\"\r\n            /></router-link>\r\n          </div>\r\n          <div class=\"product-content\">\r\n            <div class=\"product-rating\">\r\n              <i class=\"active icofont-star\"></i\r\n              ><i class=\"active icofont-star\"></i\r\n              ><i class=\"active icofont-star\"></i\r\n              ><i class=\"active icofont-star\"></i><i class=\"icofont-star\"></i\r\n              ><a href=\"product-video.html\">(3)</a>\r\n            </div>\r\n            <h6 class=\"product-name\">\r\n              <router-link\r\n                :to=\"{\r\n                  name: 'SingleProduct',\r\n                  params: { slug: product.slug },\r\n                }\"\r\n                >{{ product.name }}</router-link\r\n              >\r\n            </h6>\r\n            <h6 class=\"product-price\" v-if=\"product.offer_price\">\r\n              <del>{{ $filters.currencyBDT(product.price) }}</del\r\n              ><span>{{ $filters.currencyBDT(product.offer_price) }}</span>\r\n            </h6>\r\n            <h6 class=\"product-price\" v-else>\r\n              <span>{{ $filters.currencyBDT(product.price) }}</span>\r\n            </h6>\r\n\r\n            <button\r\n              class=\"product-add\"\r\n              title=\"Add to Cart\"\r\n              data-bs-toggle=\"modal\"\r\n              data-bs-target=\"#product-view\"\r\n              @click.prevent=\"showProductModal(product)\"\r\n            >\r\n              <i class=\"fas fa-shopping-basket\"></i><span>Shop Now</span>\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  props: [\"products\"],\r\n\r\n  data() {\r\n    return {\r\n      productSearch: \"\"\r\n    }\r\n  },\r\n  computed: {\r\n    user() {\r\n      return this.$store.getters[\"auth/GET_AUTH_INFO\"];\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style></style>\r\n"],"sourceRoot":""}]}