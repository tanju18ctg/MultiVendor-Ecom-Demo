M,WAAA,CAYG,EAPF,mBAAA,CAMO,QANP,EAMO;AALP,MAAA,KAAK,EAAC,EAKC;AAJP,MAAA,KAAK,EAAC,eAIC;AAHN,MAAA,OAAK,EAAA,cAAA,CAAA,UAAA,MAAA;AAAA,eAAU,QAAA,CAAA,aAAA,CAAc,IAAd,CAAV;AAAA,OAAA,EAA4B,CAAA,SAAA,CAA5B;AAGC,KANP,E,WAAA,E;;AAAA,M,WAAA,CAOE,CAnBN,CAwBI,EAJJ,mBAAA,CAGK,IAHL,EAGK,IAHL,EAGK,gBAAA,CAFA,MAAA,CAAA,QAEA,CAAA,GAFQ,gBAAA,CAAA,CACL,IAAI,CAAC,KAAL,GAAa,IAAI,CAAC,QADb,EACuB,cADvB,EAAA,CADb,EAEkD;AAAA;AAFlD,KAII,CAzBN,CA0BI,CAzCN,C,CAhBF,C;GA0DK,CA1DL,E;;AAAA,GA2DG,EA5DL,C,mBA8DA,mBAAA,CAKK,IALL,EAAA,WAAA,EAKK,WALL,C,CAuBM,E,CAhB0B,MAAA,CAAA,U,IAAU,C,kBAA1C,mBAAA,CAeM,KAfN,EAAA,WAAA,EAeM,CAdJ,mBAAA,CAAA,yTAAA,CAcI,EANJ,YAAA,CAKC,sBALD,EAKC;AALa,IAAA,EAAE,EAAE;AAAA,MAAA,IAAA,EAAA;AAAA,KAKjB;AALqC,IAAA,KAAK,EAAC,mBAK3C;AAL+D,IAAA,IAAI,EAAC;AAKpE,GALD,E;sBACG;AAAA,aACA,CADA,WACA,EAAA,mBAAA,CAEA,MAFA,EAAA,WAAA,EAEA,gBAAA,CADK,MAAA,CAAA,QACL,CAAA,GADa,gBAAA,CAAM,MAAA,CAAA,UAAA,CAAW,cAAX,EAAN,CADb,EAC4C;AAAA;AAD5C,OAAA,CADA;AAAA,K;;;;GADH,CAMI,CAfN,C,qCAgBM,CAlGR,CAmGI,CApGN,C","sourcesContent":["<template>\r\n  <div>\r\n    <aside class=\"cart-sidebar\">\r\n      <div class=\"cart-header\">\r\n        <div class=\"cart-total\" v-if=\"!totalPrice == 0\">\r\n          <i class=\"fas fa-shopping-basket\"></i\r\n          ><span>total item ({{ cartItemCount }})</span>\r\n        </div>\r\n\r\n        <div class=\"cart-total\" v-else>\r\n          <i class=\"fas fa-shopping-basket\"></i><span>Empty Cart</span>\r\n        </div>\r\n\r\n        <button class=\"cart-close\"><i class=\"icofont-close\"></i></button>\r\n      </div>\r\n      <ul class=\"cart-list\" v-if=\"!totalPrice == 0\">\r\n        <li\r\n          class=\"cart-item\"\r\n          style=\"padding-left: 10px\"\r\n          v-for=\"cart in carts\"\r\n          :key=\"cart.id\"\r\n        >\r\n          <!-- <input type=\"checkbox\" class=\"cart-checkbox\" /> -->\r\n          <div class=\"cart-media\">\r\n            <a href=\"#\" v-if=\"cart.thumbnail\">\r\n              <!-- <dynamic-img :path=\"cart.thumbnail\"/> -->\r\n              <img :src=\"`${url + cart.thumbnail}`\" alt=\"product\" />\r\n            </a>\r\n            <button class=\"cart-delete\" @click.prevent=\"deleteCart(cart)\">\r\n              <i class=\"far fa-trash-alt\"></i>\r\n            </button>\r\n          </div>\r\n          <div class=\"cart-info-group\">\r\n            <div class=\"cart-info\">\r\n              <h6>\r\n                <a href=\"product-single.html\">{{ cart.name }}</a>\r\n              </h6>\r\n\r\n              <span style=\"float: right; margin-top: -30px\">{{\r\n                cart.size\r\n              }}</span>\r\n\r\n              <span style=\"float: right\">{{ cart.color }}</span>\r\n              <p>\r\n                Unit Price - {{ currency }}{{ cart.price.toLocaleString() }}\r\n              </p>\r\n            </div>\r\n            <div class=\"cart-action-group\">\r\n              <div class=\"product-action\">\r\n                <button\r\n                  class=\"\"\r\n                  title=\"Quantity Minus\"\r\n                  @click.prevent=\"decrementCart(cart)\"\r\n                >\r\n                  <i class=\"icofont-minus\"></i></button\r\n                ><input\r\n                  title=\"Quantity Number\"\r\n                  type=\"text\"\r\n                  disabled\r\n                  :value=\"cart.quantity\"\r\n                /><button\r\n                  class=\"\"\r\n                  title=\"Quantity Plus\"\r\n                  @click.prevent=\"incrementCart(cart)\"\r\n                >\r\n                  <i class=\"icofont-plus\"></i>\r\n                </button>\r\n              </div>\r\n              <h6>\r\n                {{ currency\r\n                }}{{ (cart.price * cart.quantity).toLocaleString() }}\r\n              </h6>\r\n            </div>\r\n          </div>\r\n        </li>\r\n      </ul>\r\n\r\n      <ul class=\"cart-list\" v-else>\r\n        <li class=\"cart-item\" style=\"margin-top: 100px\">\r\n          <img src=\"/website/images/cart/cart.png\" alt=\"\" style=\"width: 50%\" />\r\n        </li>\r\n        <h3 style=\"text-align: center\">Your Cart is Empty</h3>\r\n      </ul>\r\n\r\n      <div class=\"cart-footer\" v-if=\"!totalPrice == 0\">\r\n        <!-- <button class=\"coupon-btn\">Do you have a coupon code?</button>\r\n        <form class=\"coupon-form\">\r\n          <input type=\"text\" placeholder=\"Enter your coupon code\" /><button\r\n            type=\"submit\"\r\n          >\r\n            <span>apply</span>\r\n          </button>\r\n        </form> -->\r\n        <router-link :to=\"{name: 'Checkout'}\" class=\"cart-checkout-btn\" href=\"checkout.html\"\r\n          ><span class=\"checkout-label\">Proceed to Checkout</span\r\n          ><span class=\"checkout-price\"\r\n            >{{ currency }}{{ totalPrice.toLocaleString() }}</span\r\n          ></router-link\r\n        >\r\n      </div>\r\n    </aside>\r\n  </div>\r\n</template>\r\n<script>\r\n// import DynamicImg from \"../frontend/DynamicImg.vue\";\r\nexport default {\r\n  // components:{DynamicImg},\r\n  props: [\"carts\", \"totalPrice\", \"currency\", \"cartItemCount\"],\r\n\r\n  methods: {\r\n    incrementCart(product) {\r\n      this.$store.dispatch(\"cart/ADD_OR_INCREMENT_CART\", product);\r\n    },\r\n\r\n    decrementCart(product) {\r\n      this.$store.dispatch(\"cart/DECREMENT_CART\", product);\r\n    },\r\n\r\n    deleteCart(product) {\r\n      this.$store.dispatch(\"cart/DELETE_CART\", product);\r\n    },\r\n  },\r\n\r\n  computed: {\r\n    url() {\r\n      return \"http://127.0.0.1:8000\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style></style>\r\n"],"sourceRoot":""}]}